{
  "manifest_version": 2,
  "version": "1",
  "name": "github+pr+travis",
  "description": "Display build status on github projects pull request list.",

  "icons": {
    "16":  "icon-16.png",
    "48":  "icon-48.png",
    "128": "icon-128.png"
  },

  "content_scripts": [ {
    "js":      [ "jquery-2.1.0-beta3.js", "script.js" ],
    "matches": [ "https://github.com/*" ],
    "run_at":  "document_idle"
  } ]
}
